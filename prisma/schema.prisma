// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql" // any db that has array support, not sqlite  
    url      = env("DATABASE_URL")
}

model titles {
    title_id        String  @id
    title_type      String
    primary_title   String
    original_title  String
    is_adult        Boolean
    start_year      Int
    end_year        Int
    runtime_minutes Int
}

